  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "env-cmd -f .env.prod ts-node src/index.ts",
    "dev": "env-cmd -f .env.dev nodemon src/index.ts",
    "dev:node": "env-cmd -f .env.dev ts-node src/index.ts",
    "docker-compose-down": "docker-compose -f ./docker-compose.dev.yml down",
    "docker-compose-up:dev": "docker-compose -f ./docker-compose.dev.yml up -d --build server",
    "docker:dev": "yarn docker-compose-down & yarn docker-compose-up:dev",
    "docker:logServer": "docker-compose -f ./docker-compose.dev.yml logs server"
  },
